/**
 * Test that the link-libs target property is working correctly.
 * The failure for this test is failure to compile.
 */
target C {
    link-libs: ["m", "pthread"],
    timeout: 0 sec
};

reactor Foo {
    preamble {=
        #include <pthread.h>
        #include <math.h>
    =}
    reaction (startup) {=
        info_print("Maximum of 42.0 and 75 is %.2f", fmax(4.20, 75));
    =}
}

main reactor {
    f = new Foo();
}
